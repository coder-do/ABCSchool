{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiSettings": {
    "BaseApiUrl": "https://localhost:7099/",
    "TokenEndpoints": {
      "Login": "api/token/login",
      "RefreshToken": "api/token/refresh-token"
    },
    "UserEndpoints": {
      "Update": "api/users/update",
      "ResetPassword": "api/users/change-password",
      "ById": "api/users/",
      "All": "api/users/all",
      "Register": "api/users/register",
      "RolesById": "api/users/user-roles/",
      "UpdateRoles": "api/users/update-roles/",
      "UpdateStatus": "api/users/update-status"
    },
    "RoleEndpoints": {
      "Create": "api/roles/add",
      "Update": "api/roles/update",
      "PartialById": "api/roles/partial/",
      "FullById": "api/roles/full/",
      "All": "api/roles/all",
      "Delete": "api/roles/delete/",
      "UpdatePermissions": "api/roles/update-permissions"
    },
    "TenantEndpoints": {
      "Create": "api/tenants/add",
      "Upgrade": "api/tenants/upgrade",
      "All": "api/tenants/all",
      "ById": "api/tenants/",
      "Activate": "api/tenants/",
      "DeActivate": "api/tenants/"
    },
    "SchoolEndpoints": {
      "Create": "api/schools/add",
      "Update": "api/schools/update",
